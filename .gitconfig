[push]
	default = simple
[alias]
  pretty = log --graph --pretty=format:'%G? %Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
  cleanup = "!git branch --merged | grep -v '\\*' | xargs -n 1 git branch -d"
[core]
  editor = code
